# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Discord Sync Configuration (for web app)
# The webhook URL where your Discord bot is listening (on the Pi)
DISCORD_BOT_WEBHOOK_URL=https://bot.yourdomain.com/webhook/sync
# Shared secret between web app and Discord bot for webhook authentication
DISCORD_WEBHOOK_SECRET=generate-a-secure-random-string-here

# OAuth Configuration (optional)
# GitHub OAuth
GITHUB_CLIENT_ID=your-github-client-id
GITHUB_CLIENT_SECRET=your-github-client-secret

# Google OAuth
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Discord OAuth
DISCORD_CLIENT_ID=your-discord-client-id
DISCORD_CLIENT_SECRET=your-discord-client-secret