{
  "name": "nrghax-discord-bot",
  "version": "1.0.0",
  "description": "NRGhax Discord Bot - Your life hacker friend for energy optimization",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "deploy-commands": "tsx src/deploy-commands.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "keywords": [
    "discord",
    "bot",
    "nrghax",
    "energy",
    "life-hacking"
  ],
  "author": "NRGhax Team",
  "license": "MIT",
  "dependencies": {
    "@slack/bolt": "^4.4.0",
    "@slack/web-api": "^7.10.0",
    "@supabase/supabase-js": "^2.39.3",
    "@types/express": "^5.0.3",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "puppeteer": "^24.20.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.56.0",
    "happy-dom": "^18.0.1",
    "prettier": "^3.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}
